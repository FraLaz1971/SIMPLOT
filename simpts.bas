10  REM CREATE POINTS
90  REM READ LINE NUMBER TO PUT DATA
100 READ LN
110 READ NPTS
120 READ AC
130 DIM XA(NPTS), YA(NPTS)
140 LET XMINP = 65535: LET YMINP = 65535
150 LET XMAXP = 0: LET YMAXP = 0
180 REM DEFINE THE FUNCTION TO PLOT
190 DEF FNF1(A) = A^3-2^A
195 REM PRINT THE DATA IN ASCII FILE SOURCE FORMAT
200 OPEN "POINTS.BAS" FOR OUTPUT AS #3
210 PRINT #3, LN; " DATA ";NPTS
220 FOR I = 1 TO NPTS/10
225     PRINT #3, LN+I*10; " DATA";
230     FOR J = 1 TO 9
231         XA((I-1)+J) = AC*(10*(I-1)+J): YA((I-1)+J) = FNF1(AC*(10*(I-1)+J))
232         IF XA((I-1)+J) > XMAXP THEN LET XMAXP = XA((I-1)+J)
233         IF XA((I-1)+J) < XMINP THEN LET XMINP = XA((I-1)+J)
234         IF FNF1(XA((I-1)+J)) > YMAXP THEN LET YMAXP = FNF1(XA((I-1)+J))
235         IF FNF1(XA((I-1)+J)) < YMINP THEN LET YMINP = FNF1(XA((I-1)+J))
240         PRINT #3, XA((I-1)+J);",";YA((I-1)+J);",";
245     NEXT J
247     XA((I-1)+J) = AC*(10*(I-1)+J): YA((I-1)+J) = FNF1(AC*(10*(I-1)+J))
248     IF XA((I-1)+J) > XMAXP THEN LET XMAXP = XA((I-1)+J)
249     IF XA((I-1)+J) < XMINP THEN LET XMINP = XA((I-1)+J)
250     IF FNF1(XA((I-1)+J)) > YMAXP THEN LET YMAXP = FNF1(XA((I-1)+J))
251     IF FNF1(XA((I-1)+J)) < YMINP THEN LET YMINP = FNF1(XA((I-1)+J))
252     PRINT #3, XA((I-1)+J);",";YA((I-1)+J);
260     PRINT #3, " "
270     LN = LN+I*10
290 NEXT I
350 LET XTIK = 5
360 LET YTIK = 7
600 PRINT #3, LN+I*10; " DATA ", XMINP;",";XMAXP;",";XTIK
610 LET I = I + 1
620 PRINT #3, LN+I*10; " DATA ", YMINP;",";YMAXP;",";YTIK
630 LET I = I + 1
640 PRINT #3, LN+I*10; " DATA n"
650 LET I = I + 1
660 PRINT #3, LN+I*10; " DATA A^3-2^A"
730 CLOSE #3
740 END
